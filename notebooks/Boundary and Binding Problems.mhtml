</span><span><span class="hljs-comment"># Simulation loop</span></span><span>
</span><span><span class="hljs-keyword">for</span></span><span> t </span><span><span class="hljs-keyword">in</span></span><span> </span><span><span class="hljs-built_in">range</span></span><span>(T):
    </span><span><span class="hljs-comment"># Update scalar field (diffusion + advection)</span></span><span>
    adv_x = np.roll(Phi, -</span><span><span class="hljs-number">1</span></span><span>, axis=</span><span><span class="hljs-number">1</span></span><span>) - np.roll(Phi, </span><span><span class="hljs-number">1</span></span><span>, axis=</span><span><span class="hljs-number">1</span></span><span>)
    adv_y = np.roll(Phi, -</span><span><span class="hljs-number">1</span></span><span>, axis=</span><span><span class="hljs-number">0</span></span><span>) - np.roll(Phi, </span><span><span class="hljs-number">1</span></span><span>, axis=</span><span><span class="hljs-number">0</span></span><span>)
    Phi += dt * (</span><span><span class="hljs-number">0.1</span></span><span> * laplacian(Phi) - </span><span><span class="hljs-number">0.05</span></span><span> * (vx * adv_x + vy * adv_y))

