</span><span><span class="hljs-comment"># Distance matrix</span></span><span>
dist = cdist(coords, coords)

</span><span><span class="hljs-comment"># Define coupling kernel</span></span><span>
</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">get_kernel</span></span><span>(</span><span><span class="hljs-params">dist, kind=<span class="hljs-string">'gaussian'</span></span></span><span>, sigma=</span><span><span class="hljs-number">0.3</span></span><span>):
    </span><span><span class="hljs-keyword">if</span></span><span> kind == </span><span><span class="hljs-string">'gaussian'</span></span><span>:
        </span><span><span class="hljs-keyword">return</span></span><span> np.exp(-dist**</span><span><span class="hljs-number">2</span></span><span> / (</span><span><span class="hljs-number">2</span></span><span> * sigma**</span><span><span class="hljs-number">2</span></span><span>))
    </span><span><span class="hljs-keyword">elif</span></span><span> kind == </span><span><span class="hljs-string">'mexican_hat'</span></span><span>:
        r2 = dist**</span><span><span class="hljs-number">2</span></span><span>
        </span><span><span class="hljs-keyword">return</span></span><span> (</span><span><span class="hljs-number">1</span></span><span> - r2 / sigma**</span><span><span class="hljs-number">2</span></span><span>) * np.exp(-r2 / (</span><span><span class="hljs-number">2</span></span><span> * sigma**</span><span><span class="hljs-number">2</span></span><span>))
    </span><span><span class="hljs-keyword">elif</span></span><span> kind == </span><span><span class="hljs-string">'uniform'</span></span><span>:
        </span><span><span class="hljs-keyword">return</span></span><span> np.ones_like(dist)
    </span><span><span class="hljs-keyword">else</span></span><span>:
        </span><span><span class="hljs-keyword">raise</span></span><span> ValueError(</span><span><span class="hljs-string">"Unknown kernel type"</span></span><span>)

