</span><span><span class="hljs-keyword">import</span></span><span> matplotlib.pyplot </span><span><span class="hljs-keyword">as</span></span><span> plt
</span><span><span class="hljs-keyword">from</span></span><span> sklearn.gaussian_process </span><span><span class="hljs-keyword">import</span></span><span> GaussianProcessRegressor
</span><span><span class="hljs-keyword">from</span></span><span> sklearn.gaussian_process.kernels </span><span><span class="hljs-keyword">import</span></span><span> RBF, ConstantKernel </span><span><span class="hljs-keyword">as</span></span><span> C

</span><span><span class="hljs-comment"># True scalar field (Φ) to be learned</span></span><span>
</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">true_phi</span></span><span>(</span><span><span class="hljs-params">x</span></span><span>):
    </span><span><span class="hljs-keyword">return</span></span><span> np.sin(x) * np.exp(-</span><span><span class="hljs-number">0.1</span></span><span> * x**</span><span><span class="hljs-number">2</span></span><span>)

</span><span><span class="hljs-comment"># Training data</span></span><span>
X_train = np.atleast_2d(np.linspace(-</span><span><span class="hljs-number">4.5</span></span><span>, </span><span><span class="hljs-number">4.5</span></span><span>, </span><span><span class="hljs-number">15</span></span><span>)).T
y_train = true_phi(X_train).ravel() + np.random.normal(</span><span><span class="hljs-number">0</span></span><span>, </span><span><span class="hljs-number">0.05</span></span><span>, X_train.shape[</span><span><span class="hljs-number">0</span></span><span>])  </span><span><span class="hljs-comment"># noisy observations</span></span><span>

</span><span><span class="hljs-comment"># Define GP kernel</span></span><span>
kernel = C(</span><span><span class="hljs-number">1.0</span></span><span>) * RBF(length_scale=</span><span><span class="hljs-number">1.0</span></span><span>)

</span><span><span class="hljs-comment"># Fit GP</span></span><span>
gp = GaussianProcessRegressor(kernel=kernel, alpha=</span><span><span class="hljs-number">1e-2</span></span><span>, n_restarts_optimizer=</span><span><span class="hljs-number">10</span></span><span>)
gp.fit(X_train, y_train)

</span><span><span class="hljs-comment"># Evaluation grid</span></span><span>
X = np.atleast_2d(np.linspace(-</span><span><span class="hljs-number">5</span></span><span>, </span><span><span class="hljs-number">5</span></span><span>, </span><span><span class="hljs-number">1000</span></span><span>)).T
y_pred, sigma = gp.predict(X, return_std=</span><span><span class="hljs-literal">True</span></span><span>)

</span><span><span class="hljs-comment"># RSVP-style fields</span></span><span>
phi = y_pred
v = np.gradient(phi.flatten(), X.flatten())  </span><span><span class="hljs-comment"># vector field = ∇Φ</span></span><span>
S = sigma  </span><span><span class="hljs-comment"># entropy field = uncertainty</span></span><span>

</span><span><span class="hljs-comment"># Plot</span></span><span>
plt.figure(figsize=(</span><span><span class="hljs-number">12</span></span><span>, </span><span><span class="hljs-number">8</span></span><span>))
plt.plot(X, true_phi(X), </span><span><span class="hljs-string">'r--'</span></span><span>, label=</span><span><span class="hljs-string">"True Φ(x)"</span></span><span>)
plt.plot(X_train, y_train, </span><span><span class="hljs-string">'ko'</span></span><span>, label=</span><span><span class="hljs-string">"Training points"</span></span><span>)
plt.plot(X, phi, </span><span><span class="hljs-string">'b-'</span></span><span>, label=</span><span><span class="hljs-string">"Predicted Φ(x)"</span></span><span>)
plt.fill_between(X.flatten(), phi.flatten() - S, phi.flatten() + S,
                 color=</span><span><span class="hljs-string">'blue'</span></span><span>, alpha=</span><span><span class="hljs-number">0.2</span></span><span>, label=</span><span><span class="hljs-string">'Entropy field 𝒮(x)'</span></span><span>)
plt.quiver(X[::</span><span><span class="hljs-number">40</span></span><span>], phi[::</span><span><span class="hljs-number">40</span></span><span>], </span><span><span class="hljs-number">0</span></span><span>, v[::</span><span><span class="hljs-number">40</span></span><span>], color=</span><span><span class="hljs-string">'green'</span></span><span>, angles=</span><span><span class="hljs-string">'xy'</span></span><span>, scale_units=</span><span><span class="hljs-string">'xy'</span></span><span>,
           scale=</span><span><span class="hljs-number">1</span></span><span>, width=</span><span><span class="hljs-number">0.003</span></span><span>, label=</span><span><span class="hljs-string">'Vector field 𝒗(x)'</span></span><span>)

plt.title(</span><span><span class="hljs-string">"RSVP Scalar (Φ), Vector (𝒗), and Entropy (𝒮) Fields via GPR"</span></span><span>)
plt.xlabel(</span><span><span class="hljs-string">"x"</span></span><span>)
plt.ylabel(</span><span><span class="hljs-string">"Field Value"</span></span><span>)
plt.grid(</span><span><span class="hljs-literal">True</span></span><span>)
plt.legend()
plt.tight_layout()
plt.show()
</span></span></code></div></div></pre>
